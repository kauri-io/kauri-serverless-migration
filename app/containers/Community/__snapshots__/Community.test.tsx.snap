                                            author={
                                              Object {
                                                "__typename": "PublicUserDTO",
                                                "avatar": "https://api.kauri.io:443/ipfs/QmUR61QiSMk1XqBaAyvA7WmgVBP44x7RQMBa8RU3DCMp6f",
                                                "id": "824e8fa64ff92b859cc9991ac044e00b665b9b04",
                                                "name": "Felipe Faraggi",
