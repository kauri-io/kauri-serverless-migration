                                            author={
                                              Object {
                                                "__typename": "PublicUserDTO",
                                                "avatar": "https://api.dev2.kauri.io:443/ipfs/QmXa9LzhDV4wiF9YLsv2Z9CBUaSU6ph8QLNRLTDAUBsrA8",
                                                "id": "f0f15cedc719b5a55470877b0710d5c7816916b1",
