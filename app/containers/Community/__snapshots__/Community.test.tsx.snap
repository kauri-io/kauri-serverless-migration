                                              Object {
                                                "__typename": "PublicUserDTO",
                                                "avatar": "https://api.beta.kauri.io:443/ipfs/QmekAbiDvz3Bc5y4pZB7Gnk5Zgn5iaa5CxQoSmTsDoPkP9",
