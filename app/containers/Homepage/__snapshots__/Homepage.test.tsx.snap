                              "author": Object {
                                "__typename": "PublicUserDTO",
                                "avatar": "https://api.kauri.io:443/ipfs/QmSd9oZW5JDp8tWXF3nRJYmAcHYWnJ5LniLQj9zhmvgUaB",
                                "id": "eefb9234302128259d46ed9e223fbc48b5edb5d1",
                                "name": "Julien Béranger",
