                                          author={
                                            Object {
                                              "__typename": "PublicUserDTO",
                                              "avatar": "https://api.kauri.io:443/ipfs/QmRPSgyQWCb3UAX7E6s8egtyNUNJVTxQvU1946VSegs4VX",
                                              "id": "afc2f2bbd4173311be60a7f5d4103b098d2703e8",
                                              "name": "Coogan",
                                              "username": "cooganb",
