                                                    <Connect(ArticleCard)
                                                      __typename="ArticleDTO"
                                                      attributes={
                                                        Object {
                                                          "background": "https://api.kauri.io:443/ipfs/QmVZqfLDgdnJkKTsLSPiyvEatZuXonBqdmb94mcnbaRspi",
                                                        }
                                                      }
                                                      author={
                                                        Object {
                                                          "__typename": "PublicUserDTO",
                                                          "avatar": null,
                                                          "id": "970991bdd61aed3c2cf91e9bf6944960561dbcf5",
                                                          "name": "Fred Blauer",
