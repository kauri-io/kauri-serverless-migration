                                          __typename="ArticleDTO"
                                          attributes={
                                            Object {
                                              "background": "https://api.kauri.io:443/ipfs/QmQ6Ja45gL89wV6vxiiqtBGzyShj3Q4LCe7bphvBjWLLoM",
